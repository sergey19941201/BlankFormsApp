<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BlankFormsApp.Pages.NavigationCommonPage"
             Title = "NavigationCommonPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Forward" Clicked="GoToForward" />
            <Button Text="Back" Clicked="BackButton_Click" />
            <Label x:Name="StackLabel"/>
            <Label x:Name="DataFromAnotherPageLabel"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>